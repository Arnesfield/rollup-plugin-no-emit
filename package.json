{
  "name": "rollup-plugin-no-emit",
  "version": "0.0.1",
  "description": "Skip emit for generated bundles.",
  "keywords": [
    "rollup-plugin",
    "no-emit"
  ],
  "homepage": "https://github.com/Arnesfield/rollup-plugin-no-emit#readme",
  "bugs": {
    "url": "https://github.com/Arnesfield/rollup-plugin-no-emit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Arnesfield/rollup-plugin-no-emit.git"
  },
  "license": "MIT",
  "author": "Jefferson Rylee <rylee.jeff385@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "import": "./lib/index.mjs",
    "require": "./lib/index.cjs",
    "default": "./lib/index.mjs"
  },
  "main": "lib/index.cjs",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "rollup -c",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "start": "npm run build -- -w",
    "test": "rollup -c rollup-test.config.js"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.5",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "chai": "^4.3.7",
    "eslint": "^8.42.0",
    "mocha": "^10.2.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.24.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "rollup": "^1.20.0 || ^2.0.0"
  }
}
